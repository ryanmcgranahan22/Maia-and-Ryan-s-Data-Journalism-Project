<section id="data info">

  <table id="myTable">
      <tr>
        <th>State</th>
        <th>Percent</th>


      </tr>
      <% for(let i=1; i<data.length; i++){ %>
    <tr>
      <% let num = "micro"+(data[i]["number"])+".html" %>
      <td>  <a href=<%=num%>> <%= data[i].state %></a> </td>
      <td><%= data[i].ratio %></td>
    </tr>
    <%}%>

    </table>
  </section>

    <%- include ('global/footer');  %>



