<script>

  var ultimateColors =
    ['rgb(191, 54, 54)', 'rgb(46, 0, 0)' ,'rgb(168, 0, 0)', 'rgb(255, 0, 0)','rgb(158, 27, 27)', 'rgb(207, 62, 62)', 'rgb(214, 99, 99)', 'rgb(184, 112, 112)', 'rgb(212, 146, 146)', 'rgb(227, 179, 179)',  'rgb(240, 221, 221)'
    ]
;

for(var i=2;i<10;i++){
var pie = [{
  values: [document.getElementById("microtable").rows[1].cells[4].innerHTML, document.getElementById("microtable").rows[1].cells[5].innerHTML, document.getElementById("microtable").rows[1].cells[6].innerHTML, document.getElementById("microtable").rows[1].cells[7].innerHTML,
