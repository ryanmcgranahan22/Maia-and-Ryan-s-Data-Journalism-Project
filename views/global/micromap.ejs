  labels: ['Firearm','Cut/Pierce','Struck by/against','Poisoning','Hanging/Suffocation','Fall','Drowning','Fire/Burn','Motor vehicle','Other','Unknown'],
  type: 'pie',
  marker: {
     colors: ultimateColors
   },
 }];

var layout = {
  height: 500,
  width: 700,
  paper_bgcolor: '#e9edf2',
};

Plotly.newPlot('myDiv', pie, layout);
}



